cmake_minimum_required(VERSION 3.30)
@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/rpi-qpudsl-targets.cmake")

get_target_property(_rpidsl_configs rpi-qpudsl::compiler IMPORTED_CONFIGURATIONS)
if (RELEASE IN_LIST _rpidsl_configs)
    get_target_property(_rpidsl_exe rpi-qpudsl::compiler IMPORTED_LOCATION_RELEASE)
elseif (_rpidsl_configs)
    list(GET _rpidsl_configs 0 _rpidsl_config)
    get_target_property(_rpidsl_exe rpi-qpudsl::compiler "IMPORTED_LOCATION_${_rpidsl_config}")
else ()
    set(_rpidsl_exe "-NOTFOUND")
endif ()

set_and_check(rpi_qpudsl_EXECUTABLE "${_rpidsl_exe}")

check_required_components(rpi-qpudsl)
